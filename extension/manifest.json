{
  "manifest_version": 3,
  "name": "Pigeon and Cursor Extension",
  "version": "1.0",
  "description": "A Chrome Extension with a pigeon and custom cursor interaction.",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "script.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
